{
  "name": "cat",
  "version": 2,
  "alias": "cat.dennisjefferson.com",
  "env": {
    "NODE_ENV": "@node-env",
    "CDN_URL": "@cdn-url",
    "CAT_API_URL": "@cat-api-url",
    "CAT_API_KEY": "@cat-api-key",
    "DATABASE": "@database",
    "PORT": "@port",
    "SECRET": "@secret"
  },
  "builds": [
    {
      "src": "dist/server/index.js",
      "use": "@now/node-server"
    },
    {
      "src": "dist/bundle.js",
      "use": "@now/static"
    },
    {
      "src": "dist/styles.css",
      "use": "@now/static"
    }
  ],
  "regions": ["sfo1"],
  "routes": [
    {
      "src": "/.*",
      "dest": "dist/server/index.js"
    },
    {
      "src": "/styles.css",
      "methods": ["GET"],
      "status": 200,
      "dest": "/dist/styles.css",
      "headers": { "content-type": "text/css" }
    },
    {
      "src": "/bundle.js",
      "methods": ["GET"],
      "status": 200,
      "dest": "/dist/bundle.js",
      "headers": { "content-type": "text/javascript" }
    }
  ]
}
